#:kivy 1.0.9

<Arena>:
    size: 800,600
    canvas:
        Color:
            rgb: 0.3, 0.1, 0.1
        Rectangle:
            pos: self.pos
            size: self.size

<Indicator>:
    canvas:
        Color:
            rgb: self.color
        Ellipse:
            pos: self.pos
            size: 10, 10


